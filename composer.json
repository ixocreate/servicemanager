{
    "name": "kiwi-suite/servicemanager",
    "description": "kiwi-suite/servicemanager is a psr-11 container library",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Payer"
        }
    ],
    "require": {
        "php": "^7.1",
        "zendframework/zend-servicemanager": "^3.3",
        "ocramius/proxy-manager": "^2.1",
        "zendframework/zend-di": "^3.0",
        "zendframework/zend-code": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.5",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "KiwiSuite\\ServiceManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KiwiSuiteMisc\\ServiceManager\\": "tests/misc/",
            "KiwiSuiteTest\\ServiceManager\\": "tests/src/"
        }
    }
}
