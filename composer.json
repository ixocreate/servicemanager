{
    "name": "ixocreate/servicemanager",
    "description": "IXOCREATE Servicemanager",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "zendframework/zend-servicemanager": "^3.3",
        "ocramius/proxy-manager": "=2.1.1",
        "zendframework/zend-di": "=3.0",
        "zendframework/zend-code": "^3.3"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\ServiceManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\ServiceManager\\": "tests/misc/",
            "Ixocreate\\Test\\ServiceManager\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
