{
    "name": "ixocreate/servicemanager",
    "description": "IXOCREATE Servicemanager Library",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE",
            "email": "opensource@ixocreate.com",
            "homepage": "https://www.ixocreate.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ocramius/proxy-manager": "=2.1.1",
        "zendframework/zend-di": "^3.1",
        "zendframework/zend-servicemanager": "^3.3"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^2.0",
        "ixocreate/test": "dev-develop",
        "vimeo/psalm": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\ServiceManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\ServiceManager\\": "tests/misc/",
            "Ixocreate\\Test\\ServiceManager\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
